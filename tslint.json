{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [true, "single"],
        "max-line-length": false,
        "no-console": false,
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "ordered-imports": false,
        "one-line": false,
        "triple-equals": false
    },
    "rulesDirectory": []
}